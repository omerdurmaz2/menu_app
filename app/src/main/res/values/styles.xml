<?xml version="1.0" encoding="utf-8"?>
<resources>



    <style name="TextviewMarckRegularStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/marck_regular</item>
        <item name="android:textSize">@dimen/dg_text_size_26</item>
    </style>


    <style name="TextViewRobotoBoldStyle">
        <item name="lineHeight">24dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
    </style>


    <style name="TextViewRobotoRegularStyle">
        <item name="android:textColor">@color/white</item>
        <item name="lineHeight">20sp</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
    </style>


    <style name="TextViewRobotoBoldStyleThree" parent="TextViewRobotoBoldStyle">
        <item name="android:textSize">@dimen/dg_text_size_16</item>
    </style>


    <style name="TextViewRobotoRegularStyleThree" parent="TextViewRobotoRegularStyle">
        <item name="android:textSize">@dimen/dg_text_size_16</item>
    </style>


</resources>